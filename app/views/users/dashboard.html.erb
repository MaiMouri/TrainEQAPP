<div class="container p-3 my-3">
<h1 class="title">Hey, <%= current_user.email %>!</h1>
<h3 class="subtitle">This is your dashboard</h3>
<ul class="nav nav-tabs" id="myTab" role="tablist">
  <li class="nav-item" role="presentation">
    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Simple</a>
  </li>
  <li class="nav-item" role="presentation">
    <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Compare</a>
  </li>
  <li class="nav-item" role="presentation">
    <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Detailed (👷‍♀️ under construction 🚧)</a>
  </li>
</ul>
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
    <div class="chTitle">Your meeting results!</div>
    <canvas
      data-controller="chart"
      data-chart-type-value="doughnut"
      data-chart-data-value='{ "labels": ["Happy", "Surprised", "Neutral", "Angry", "Disgusted", "Sad", "Fearful"],
                              "datasets": [{ "label": "Zoom participant emotions",
                              "backgroundColor": [
                                                  "rgba(75, 192, 192, 0.9)",
                                                  "rgba(255, 159, 64, 0.9)",
                                                  "rgba(255, 205, 86, 0.9)",
                                                  "rgba(255, 99, 132, 0.9)",
                                                  "rgba(54, 162, 235, 0.9)",
                                                  "rgba(153, 102, 255, 0.9)",
                                                  "rgba(153, 132, 131, 0.9)"],
                              <%# "borderColor": "#3B82F6", %>
                              "data": [37, 13, 78, 54, 12, 5, 40] }] }'
    ></canvas>
  </div>
  <%# first parent tab end %>
  <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
    <div class="chsubTitle">Let's compare with your previous few meetings!</div>
    <div class="chsubTitle1">Take a look!</div>
      <ul class="nav nav-tabs" id="myTab" role="tablist">
        <li class="nav-item" role="presentation">
          <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Happy</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Surprised</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Neutral</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Angry</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Disgusted</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Sad</a>
        </li>
        <li class="nav-item" role="presentation">
          <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Fearful</a>
        </li>
      </ul>
      <%# individual emotions list end %>
    <canvas
      data-controller="chart"
      data-chart-type-value="bar"
      data-chart-data-value='{ "labels": ["10/26", "11/2", "11/17", "11/22", "Today"],
                              "datasets": [{ "label": "Zoom participant emotions",
                              "backgroundColor": [
                                                  "rgba(75, 192, 192, 0.9)",
                                                  "rgba(255, 159, 64, 0.9)",
                                                  "rgba(255, 205, 86, 0.9)",
                                                  "rgba(255, 99, 132, 0.9)",
                                                  "rgba(54, 162, 235, 0.9)",
                              "borderColor": "rgba(54, 162, 235, 0.2)",
                              "data": [17, 15, 24, 23, 27,] }] }'
    ></canvas>
    <%# first nested emotion tab end%>
    <canvas
      data-controller="chart"
      data-chart-type-value="bar"
      data-chart-data-value='{ "labels": ["10/26", "11/2", "11/17", "11/22", "Today"],
                              "datasets": [{ "label": "Zoom participant emotions",
                              "backgroundColor": [
                                                  "rgba(75, 192, 192, 0.9)",
                                                  "rgba(255, 159, 64, 0.9)",
                                                  "rgba(255, 205, 86, 0.9)",
                                                  "rgba(255, 99, 132, 0.9)",
                                                  "rgba(54, 162, 235, 0.9)",
                              "borderColor": "rgba(54, 162, 235, 0.2)",
                              "data": [17, 15, 24, 23, 27,] }] }'
    ></canvas>
    <%# second nested emotion tab end %>
  </div>
  <%# second parent tab end %>
  <div class="tab-pane fade" id="compare" role="tabpanel" aria-labelledby="contact-tab">
    <canvas
      data-controller="chart"
      data-chart-type-value="doughnut"
      data-chart-data-value='{ "labels": ["Happy", "Sad", "Neutral", "Angry", "Disgusted", "Surprised", "Fearful"],
                              "datasets": [{ "label": "Zoom participant emotions",
                              "backgroundColor": ["rgba(255, 99, 132, 0.9)", "rgba(255, 159, 64, 0.9)", "rgba(255, 205, 86, 0.9)", "rgba(75, 192, 192, 0.9)", "rgba(54, 162, 235, 0.9)", "rgba(153, 102, 255, 0.9)", "rgba(153, 132, 131, 0.9)"],
                              <%# "borderColor": "rgba(54, 162, 235, 0.2)", %>
                              "data":<%= @emotions %> }] }'
    ></canvas>
  </div>
  <%# third parent tab end %>
</div>




