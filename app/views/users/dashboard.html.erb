      </ul>
        <%# individual emotions list end %>
      <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="happy" role="tabpanel" aria-labelledby="happy-tab">
          <canvas
            data-controller="chart"
            data-chart-type-value="bar"
            data-chart-data-value='{ "labels": ["10/26", "11/2", "11/17", "11/22", "Today"],
                                    "datasets": [{ "label": "Zoom participant emotions",
                                    "backgroundColor": [
                                                        "rgba(75, 192, 192, 0.9)",
